    "def fib(n):\n",
    "\n",
    "    if (n < 0) :\n",
    "        print(\"Número Inválido\")\n",
    "    \n",
    "    if (n == 0):\n",
    "        return 0\n",
    "        \n",
    "    if (n == 1):\n",
    "        return 1\n",
    "    \n",
    "    return fib(n-2) + fib(n-1)\n",
    "\n",
    "print(fib(32))\n"
   ]
  }
